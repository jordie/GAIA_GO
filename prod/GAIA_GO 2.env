# GAIA_GO SELF-IMPROVEMENT (DOGFOODING) ENVIRONMENT
# This environment is used by GAIA_GO to improve itself

GAIA_VERSION=1.0.0-prod
GAIA_ENV=prod
GAIA_SELF_IMPROVE=true
GAIA_DOGFOOD=true

# Self-monitoring
GAIA_METRICS_ENABLED=true
GAIA_PROFILING_ENABLED=true
GAIA_SELF_AUDIT_ENABLED=true

# Self-improvement parameters
GAIA_CODE_ANALYSIS=true
GAIA_PERFORMANCE_OPTIMIZATION=true
GAIA_BUG_DETECTION=true
GAIA_TEST_GENERATION=true
GAIA_DOCUMENTATION_GENERATION=true

# Development parameters
GAIA_AUTO_BUILD=true
GAIA_AUTO_TEST=true
GAIA_AUTO_DEPLOY=true

# Task queue for self-improvement
GAIA_TASK_QUEUE=true
GAIA_TASK_PRIORITY=high

# Logging and diagnostics
GAIA_LOG_LEVEL=debug
GAIA_DIAGNOSTIC_MODE=true
