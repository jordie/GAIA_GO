# GAIA_GO STAGING ENVIRONMENT
# Purpose: Pre-production testing, validation, integration testing
# Stability: Must be stable, reflects prod configuration
# Database: staging_gaia.db (persistent, production-like)

GAIA_ENV=staging
GAIA_VERSION=staging-latest
GAIA_ENVIRONMENT_DIR=/Users/jgirmay/Desktop/gitrepo/pyWork/GAIA_GO/environments/staging

# Staging Server
GAIA_PORT=8082
GAIA_HOST=0.0.0.0
GAIA_LOG_LEVEL=info
GAIA_DEBUG=false

# Self-Improvement Settings
GAIA_SELF_IMPROVE=true
GAIA_AUTO_BUILD=true
GAIA_AUTO_TEST=true
GAIA_AGGRESSIVE_OPTIMIZATION=false

# Production-like Features
GAIA_EXPERIMENTAL_FEATURES=false
GAIA_CODE_PROFILING=true
GAIA_MEMORY_PROFILING=true

# Database
GAIA_DB_PATH=./data/staging_gaia.db
GAIA_DB_AUTO_MIGRATE=true
GAIA_DB_RESET_ON_ERROR=false

# Logging
GAIA_LOG_DIR=./logs
GAIA_LOG_MAX_SIZE=50M
GAIA_LOG_RETENTION_DAYS=30

# Task Queue
GAIA_TASK_QUEUE=true
GAIA_TASK_WORKER_THREADS=8
GAIA_TASK_TIMEOUT_MINUTES=30

# Metrics & Monitoring
GAIA_METRICS_ENABLED=true
GAIA_METRICS_PORT=9091
GAIA_HEALTH_CHECK_ENABLED=true

# Production Simulation
GAIA_SKIP_AUTH=false
GAIA_CORS_ALLOW_ALL=false
GAIA_VERBOSE_ERRORS=false
