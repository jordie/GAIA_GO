# GAIA_GO DEVELOPMENT ENVIRONMENT
# Purpose: Rapid development, experimentation, new features
# Stability: Not guaranteed, frequent changes
# Database: dev_gaia.db (can be reset)

GAIA_ENV=dev
GAIA_VERSION=dev-latest
GAIA_ENVIRONMENT_DIR=/Users/jgirmay/Desktop/gitrepo/pyWork/GAIA_GO/environments/dev

# Development Server
GAIA_PORT=8081
GAIA_HOST=localhost
GAIA_LOG_LEVEL=debug
GAIA_DEBUG=true

# Self-Improvement Settings
GAIA_SELF_IMPROVE=true
GAIA_AUTO_BUILD=true
GAIA_AUTO_TEST=true
GAIA_AGGRESSIVE_OPTIMIZATION=true

# Development Features (Experimental)
GAIA_EXPERIMENTAL_FEATURES=true
GAIA_CODE_PROFILING=true
GAIA_MEMORY_PROFILING=true

# Database
GAIA_DB_PATH=./data/dev_gaia.db
GAIA_DB_AUTO_MIGRATE=true
GAIA_DB_RESET_ON_ERROR=true

# Logging
GAIA_LOG_DIR=./logs
GAIA_LOG_MAX_SIZE=10M
GAIA_LOG_RETENTION_DAYS=7

# Task Queue
GAIA_TASK_QUEUE=true
GAIA_TASK_WORKER_THREADS=4
GAIA_TASK_TIMEOUT_MINUTES=5

# Metrics & Monitoring
GAIA_METRICS_ENABLED=true
GAIA_METRICS_PORT=9090
GAIA_HEALTH_CHECK_ENABLED=true

# Development Overrides
GAIA_SKIP_AUTH=true
GAIA_CORS_ALLOW_ALL=true
GAIA_VERBOSE_ERRORS=true
